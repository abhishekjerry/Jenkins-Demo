pipeline {
    agent any
    tools {nodejs "nodejs"}
    stages{
        stage("Install Stage"){
            steps {
                sh "npm install"
            }
        }
        stage("Build Stage"){
            steps {
               sh "npm run build"
            }
        }
        stage("Echo"){
            steps {
                echo "Hi from 2nd stage"
            }
        }
    }
}
